{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jsx-a11y/recommended"
    // "airbnb"
  ],
  "rules": {
    "no-console": "error",
    "curly": "error",
    "func-style": ["error", "expression"],
    "prettier/prettier": ["error", {
      "endOfLine":"auto"
     }]
  }
}

